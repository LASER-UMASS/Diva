P = 
fun e : option L => match e with
                    | Some cl => Some cl
                    | None => None
                    end
     : option L -> option L
fun n : nat => let y : T n := A n in ?t ?x : T n
     : forall n : nat, T n
where
?t : [n : nat  y := A n : T n |- ?T -> T n]
?x : [n : nat  y := A n : T n |- ?T]
fun n : nat => ?t ?x : T n
     : forall n : nat, T n
where
?t : [n : nat |- ?T -> T n]
?x : [n : nat |- ?T]
